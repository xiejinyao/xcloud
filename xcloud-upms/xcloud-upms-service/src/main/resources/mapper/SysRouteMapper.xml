<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xjinyao.xcloud.admin.mapper.SysRouteMapper">
    <resultMap id="BaseResultMap" type="com.xjinyao.xcloud.admin.api.entity.SysRoute">
        <result column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="path" property="path"/>
        <result column="url" property="url"/>
        <result column="service_id" property="serviceId"/>
        <result column="status" property="status"/>
        <result column="create_by" property="createBy"/>
        <result column="update_by" property="updateBy"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="tenant_id" property="tenantId"/>
    </resultMap>

    <resultMap id="routeVOResultMap" type="com.xjinyao.xcloud.admin.api.vo.SysRouteVO">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="service_id" property="serviceId"/>
    </resultMap>

    <select id="listItem" resultMap="routeVOResultMap">
        select id, name, service_id
        from sys_route
        order by create_time
    </select>

</mapper>
